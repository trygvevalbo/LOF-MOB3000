<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context=".FrontPage">


        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/hovedListe"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
            android:id="@+id/taptKnapp"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:layout_gravity="end|bottom"
            android:layout_marginBottom="130dp"
            android:src="@drawable/ic_tapt"
            android:text="Tapt Noe?"
            android:theme="@style/Materialtheme"
            android:visibility="invisible"
            app:backgroundTint="@color/SecondaryAccent"
            app:elevation="6dp"
            app:icon="@drawable/ic_tapt"
            app:pressedTranslationZ="12dp" />

        <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
            android:id="@+id/funnetKnapp"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:layout_gravity="end|bottom"
            android:layout_marginBottom="80dp"
            app:elevation="6dp"
            app:pressedTranslationZ="12dp"
            app:backgroundTint="@color/SecondaryAccent"
            android:src="@drawable/ic_funnet"
            android:text="Funnet Noe?"
            app:icon="@drawable/ic_funnet"
            android:visibility="invisible"
            android:theme="@style/Materialtheme"
            />

        <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
            android:id="@+id/submitButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="end|bottom"
            android:layout_margin="10dp"
            app:backgroundTint="@color/MainAccent"
            app:elevation="10dp"
            app:useCompatPadding="true"
            app:pressedTranslationZ="12dp"
            android:text="Lag Annonse"
            app:icon="@drawable/ic_add_item_light"
            app:iconSize="16dp"
            android:theme="@style/Materialtheme"
            android:textSize="12sp"
            />



</androidx.coordinatorlayout.widget.CoordinatorLayout>